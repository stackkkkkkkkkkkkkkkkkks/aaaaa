	max = 100,
	decimals = 0,
	callback = function(val)
		trigger_hitchance = val
	end
})

sections.Section2:AddSlider({
	text = "Trigger Delay â±",
	flag = "trigger_delay",
	value = 0.1,
	min = 0.1,
	max = 1,
	decimals = 2,
	callback = function(val)
		trigger_delay = val
	end
})
